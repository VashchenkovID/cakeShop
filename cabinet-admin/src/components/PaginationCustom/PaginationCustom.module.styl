.PaginationItemsFooter {
  display: flex;
  justify-content: space-between;
  padding 16px
  border-radius 10px
  background: radial-gradient(350.94% 343.49% at 51.82% -159.87%, #404657 0%, rgba(64, 70, 87, 0.00) 100%);
  backdrop-filter: blur(34px);
}

.select {
  width: 84px;
  &Container {
    display: flex;
    gap: 8px;
    width: fit-content;
    align-items: center;
    white-space: nowrap;
  }
}

.actions {
  display: flex;
  gap: 16px;
  align-items: center;

  &Page {
    background: rgba(0, 66, 105, 0.07);
    width: 24px;
    height: 24px;
    border-radius: 50%;

    display: flex;
    justify-content: center;
    align-items: center;
  }
}
