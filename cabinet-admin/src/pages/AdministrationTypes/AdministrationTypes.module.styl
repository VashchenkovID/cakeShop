.Types
  display grid
  grid-template-columns 1fr 1fr
  gap 24px
  @media (max-width 1050px)
    display flex
    flex-direction column
    max-height 75vh
    overflow-y auto
  &__column
    display flex
    flex-direction column
    gap 24px
    background: radial-gradient(350.94% 343.49% at 51.82% -159.87%, #404657 0%, rgba(64, 70, 87, 0.00) 100%);
    backdrop-filter: blur(34px);
    padding 16px
    border-radius 10px
    height calc(36vh)
.Modal
  width auto
  height auto
