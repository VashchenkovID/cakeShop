.Graph
  display flex
  flex-direction column
  gap 24px
  border-radius 10px
  background: radial-gradient(350.94% 343.49% at 51.82% -159.87%, #404657 0%, rgba(64, 70, 87, 0.00) 100%);
  backdrop-filter: blur(34px);
  padding 16px
  &__rows
    display flex
    flex-direction column
    gap 16px
    max-height 22vh
    overflow-y auto
    &__row
      display grid
      grid-template-columns repeat(5,1fr)
      gap 8px
.line
  max-width 50vw
