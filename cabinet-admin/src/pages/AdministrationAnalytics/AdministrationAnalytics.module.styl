.Analytics
  display grid
  grid-template-columns 1fr 0.7fr
  gap 24px
  &__badgesContainer
    background rgba(47, 4, 79, 0.5)
    padding 16px
    border-radius 10px
  &__badges
    display flex
    gap 24px
    justify-content space-between
    border-radius 10px
    background: radial-gradient(350.94% 343.49% at 51.82% -159.87%, #404657 0%, rgba(64, 70, 87, 0.00) 100%);
    backdrop-filter: blur(34px);
    padding 16px
    &__badge
      display flex
      flex-direction column
      gap 24px
.rightSide
  display flex
  flex-direction column
  gap 24px
